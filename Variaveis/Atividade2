let nome = prompt("Digite seu nome: ")
let idade = Number(prompt("Digite sua idade: "))
let email = prompt("Digite seu email: ")
let telefone = Number(prompt("Digite seu telefone: "))
console.log("O seu nome é", nome)
console.log("você tem", idade, "anos")
console.log("Pertence a você o email", email)
console.log("e seu numero de telefone é", telefone)
console.log("Obrigado pelas sua informações, agora podemos continuar...")
let data = prompt("Qual é o seu ano de nascimento?" )
let ano = 2025
let resultado = ano - data
console.log(resultado)
console.log("Você tem", resultado, "anos de idade.")
