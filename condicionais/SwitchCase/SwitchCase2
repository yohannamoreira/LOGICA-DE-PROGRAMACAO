let respostaCorreta = 0
let respostaIncorreta = 0

console.log("Qual real nome da cantora Carol Biazin")
console.log("A - Carol biazin")
console.log("B - Carolina Souza Biazin")
console.log("C - Caroline dos Reis Biazin")
console.log("D - Caroline Souza dos Reis Biazin")
let pergunta1 = prompt ("Digite a alternativa correta: ")
switch (true){
    case pergunta1 == "C" || pergunta1 == "Caroline dos Reis Biazin":
        console.log("Resposta correta!\n")
        respostaCorreta++
        break
        default:
        console.log("Resposta Incorreta!\n ")
        respostaIncorreta ++
        break
}
console.log("Qual programa a Carol Biazin ficou conhecida")
console.log("A - The voice Brasil")
console.log("B - Idolos")
